    // Function to show or hide the clear search icon based on input field value
    function toggleClearSearchIcon() {
        clearSearchIcon.style.display = searchInput.value.trim() !== "" ? "inline-block" : "none";
        noResultSearchIcon.style.display = searchInput.value.trim() !== "" ? "none" : "inline-block";
    }

