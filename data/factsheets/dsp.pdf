    $(document).ready(function() {
        // Function to open search popup
        $(".search-popup-link").click(function(event) {
            event.stopPropagation(); // Prevent event bubbling
            var SearchPopup = $("#" + $(this).data("popup"));
            SearchPopup.addClass("active");
