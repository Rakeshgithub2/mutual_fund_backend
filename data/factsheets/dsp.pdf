        var noResultWrap = document.querySelector(".dsp-no-result-found-wrap");

        // Function to clear the search and show the trending wrap
        function clearSearch() {
            searchInput.value = ""; // Clear the input field
            trendingWrap.style.display = "block"; // Show the trending wrap
            searchFilterMain.style.display = "none";
            listeningWrap.style.display = "none"; // Hide the search filter main
            noResultWrap.style.display = "none";
            noResultSearchIcon.style.display = "none";
        }

        // Add input event listener to the search input field
        searchInput.addEventListener("input", function() {
            debounce(toggleClearSearchIcon, 500); // Show or hide the clear search icon

            // Hide the trending wrap if there is text in the input field
            // trendingWrap.style.display = searchInput.value.trim() !== "" ? "none" : "block";

            // Show the search filter main if there is text in the input field
            if (searchInput.value.trim() === "") {
                searchFilterMain.style.display = "none";
            }
            listeningWrap.style.display = searchInput.value.trim() !== "" ? "none" : "none";
            noResultWrap.style.display = searchInput.value.trim() !== "" ? "none" : "none";

        });

        clearSearchIcon.addEventListener("click", function() {
            clearSearch(); // Clear the search
            $(this).hide();
