    // Get cookies by name
    function getCookie(name) {
        let match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
        if (match) return decodeURIComponent(match[2]);
    }

    function setCookie(name, value, days) {
        let expires = "";
        if (days) {
            let date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (encodeURIComponent(value) || "") + expires + "; path=/";
    }

    // Update the recent searches list in the DOM
    function updateRecentSearches() {
        let recentSearches = JSON.parse(getCookie('recentSearches') || '[]');
        let recentSearchList = document.getElementById('recentSearchList');
        recentSearchList.innerHTML = '';

        if (recentSearches.length === 0) {
            document.querySelector('.dsp-no-recent-search').style.display = 'block';
