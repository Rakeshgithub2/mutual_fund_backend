    function downloadURI(uri, name) 
    {
        let link = document.createElement("a");
        // If you don't know the name or want to use
        // the webserver default set name = ''
        link.setAttribute('target', '_blank');
        link.setAttribute('download', name);
        link.href = uri;
        link.download = "";
        document.body.appendChild(link);
        link.click();
        link.remove();
    }

    // intials when page loads
    let sortx = getParameterByName('sort');
    let pagex = getParameterByName('page');
    let page_limitx = getParameterByName('per_page');
    let yearx = getParameterByName('year');
    let monthx = getParameterByName('month');
    let searchx = getParameterByName('search');
    let categoryx = getParameterByName('category');
    log('categoryx = '+categoryx);
    let sub_categoryx = getParameterByName('sub_category');
    if (pagex != '') {
        page = pagex;
    }
    if (page_limitx != '' && page_limitx != undefined) {
        page_limit = page_limitx;
    }
    if (categoryx != '' && categoryx != undefined) {
        category = categoryx;
        $('.download-center-category-dropdown-item').removeClass('selected');
        $('#' + category.replace(/[^A-Z0-9]/ig, "-").toLowerCase()).addClass('selected');
        selectedButtonValue = $('.download-center-category-dropdown-item.selected').text();

        $('.download-center-category-dropdown-btn > p').html(selectedButtonValue);
        set_cats();
        if (category != 'All Categories') {
            $('.download-center-subcategory-dropdown-btn').prop('disabled',false);
            $('.download-center-subcategory-dropdown-item').attr('style', 'display:none !important;');
            $('.dcsdi-hr').attr('style', 'display:none !important;');

            $('.download-center-subcategory-dropdown-item.' + category.replace(/[^A-Z0-9]/ig, "-").toLowerCase()).show();
            $('.dcsdi-hr.' + category.replace(/[^A-Z0-9]/ig, "-").toLowerCase()).show();
        } else {
            $('.download-center-subcategory-dropdown-btn').prop('disabled', 'true');
            $('.download-center-subcategory-dropdown-item').show();
            $('.dcsdi-hr').show();
        }
    }
    if (sub_categoryx != '' && sub_categoryx != undefined) {
        sub_category = sub_categoryx;
        $('.download-center-subcategory-dropdown-item').removeClass('selected');
        $('#' + sub_category.replace(/[^A-Z0-9]/ig, "-").toLowerCase()).addClass('selected');
        selectedButtonValue = $('.download-center-subcategory-dropdown-item.selected').text();

        $('.download-center-subcategory-dropdown-btn > p').html(selectedButtonValue);
        set_cats();
    }
    if (monthx != '' && monthx != undefined) {
        month = monthx;
    }
    if (yearx != '' && yearx != undefined) {
        year = yearx;
    }
    if (searchx != '' && searchx != undefined) {
        search = searchx;
        $('#search').val(search);
        $('#search').trigger('change');
    } else {
        if (sortx != '' && sortx != undefined) {
            sort = sortx;
            if (sort == 'latest-insight') {
                set_filter('latest-insight');
                if (category != '') {
                    set_category(category.replace(/[^A-Z0-9]/ig, "-").toLowerCase());
                }
                if (sub_category != '') {
                    set_sub_category(sub_category.replace(/[^A-Z0-9]/ig, "-").toLowerCase());
                }
                $('#latest-insight').trigger('click');
            }
            if (sort == 'alp') {
                set_filter('alp');
                if (category != '') {
                    set_category(category.replace(/[^A-Z0-9]/ig, "-").toLowerCase());
                }
                if (sub_category != '') {
                    set_sub_category(sub_category.replace(/[^A-Z0-9]/ig, "-").toLowerCase());
                }
                $('#alp').trigger('click');
            }
        }
        // else if (monthx != '' && monthx != undefined && yearx != '' && yearx != undefined) {
