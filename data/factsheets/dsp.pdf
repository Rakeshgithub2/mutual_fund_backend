<script>
	document.addEventListener('DOMContentLoaded', function() {
		let recaptchaLoaded = false;
		let html2pdfLoaded = false;

		function loadScript({
			src,
			integrity,
			crossorigin,
			referrerPolicy
		}) {
			const script = document.createElement('script');
			script.src = src;
			script.async = true;
			script.defer = true;
			if (integrity) script.integrity = integrity;
			if (crossorigin) script.crossOrigin = crossorigin;
			if (referrerPolicy) script.referrerPolicy = referrerPolicy;
			document.body.appendChild(script);
		}

		function loadReCaptcha() {
			if (recaptchaLoaded) return;
			recaptchaLoaded = true;

			loadScript({
				src: 'https://www.google.com/recaptcha/api.js?render=6LcX73wkAAAAAOCmFNSvt955yKXOycah6gSwCbb6',
			});
		}

		function loadHtml2Pdf() {
			if (html2pdfLoaded) return;
			html2pdfLoaded = true;

			loadScript({
				src: 'https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js',
				integrity: 'sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg==',
				crossorigin: 'anonymous',
				referrerPolicy: 'no-referrer'
			});
		}

		function loadScriptsOnInteraction() {
			loadReCaptcha();
			loadHtml2Pdf();
		}

		['scroll', 'click', 'mousemove', 'keydown'].forEach(event => {
			window.addEventListener(event, loadScriptsOnInteraction, {
				once: true,
				passive: event === 'scroll'
			});
		});
	});
