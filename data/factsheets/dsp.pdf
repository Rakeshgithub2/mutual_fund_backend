        //     month = monthx;
        //     year = yearx;
        // }
    }

    function set_filter(id) {
        $('.download-center-sort-dropdown-item').removeClass('selected');
        $('#' + id).addClass('selected');
        selectedButtonValue = $('.download-center-sort-dropdown-item.selected').text();
        $('.download-center-sort-dropdown-btn > p').html(selectedButtonValue);
    }

    function set_category(id) {
        $('.download-center-category-dropdown-item').removeClass('selected');
        $('#' + id).addClass('selected');
        selectedButtonValue = $('.download-center-category-dropdown-item.selected').text();
        category = selectedButtonValue;
        $('.download-center-category-dropdown-btn > p').html(selectedButtonValue);
        set_cats();
        if (search != '') {
            set_params({
                'search': search,
                'category': category
            });
        } else {
            set_params({
                'sort': sort,
                'category': category
            });
        }
    }

    function set_sub_category(id) {
        $('.download-center-subcategory-dropdown-item').removeClass('selected');
        $('#' + id).addClass('selected');
        selectedButtonValue = $('.download-center-subcategory-dropdown-item.selected').text();
        sub_category = selectedButtonValue;
        $('.download-center-subcategory-dropdown-btn > p').html(selectedButtonValue);
        set_cats();
        if (search) {
            set_params({
                'search': search,
                'sub_category': sub_category
            });
        } else {
            set_params({
                'sort': sort,
                'sub_category': sub_category
            });
        }
    }

    if (sort == '' && search == '') {
        // intial page load
        $('#latest-insight').trigger('click');
        if(category == '' || category == 'All Categories'){
            $('.download-center-subcategory-dropdown-btn').prop('disabled', 'true');
        }
    }

    /**@abstract
