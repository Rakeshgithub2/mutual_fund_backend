            //height for animation
            setTimeout(() => {
                adjustHeight();
            }, 200);
            $('#coachmarkBox').hide();
        });

        trendingWrap.style.display = "block";

    });

    function resetFilters() {
        $('.scheme-category-filter').each(function() {
            $(this).removeClass('add-filter');
            $(this).closest('li').css('display', '');
        });
    }

    $(document).ready(function() {
        $('.scheme-category-filter').click(function() {

            // Count the number of filters that are not disabled (don't have the 'add-filter' class)
            const activeFilters = $('.scheme-category-filter:not(.add-filter)').filter(function() {
                return $(this).closest('li').css('display') !== 'none';
            }).length;

            // Only proceed if there is at least one active filter
            if (activeFilters > 1 || (activeFilters == 1 && $(this).hasClass('add-filter'))) {
                setTimeout(() => {
                    const event = new Event('input', {
                        bubbles: true
                    });

                    const inputElement = document.getElementById('searchPlaceholderInput2');
                    const mockEvent = {
                        target: inputElement
                    };

                    handleSearchInput(mockEvent);

                }, 100);

                $(this).toggleClass('add-filter');
            }
        });

        // Text type animation in Input Search Box
        const placeholders = [
            '"Factsheet"',
            '"Netra"',
            '"Funds"',
            '"Forms"'
        ];

        let currentIndex = 0;

        function changePlaceholder(elementId) {
            const inputElement = document.getElementById(elementId);
            if (inputElement) {

                inputElement.setAttribute('placeholder', 'Search for ' + placeholders[currentIndex]);
            }
        }

        function updatePlaceholders() {
            changePlaceholder('searchPlaceholderInput1');
            changePlaceholder('searchPlaceholderInput2');
            currentIndex = (currentIndex + 1) % placeholders.length;
        }

        updatePlaceholders();
        setInterval(updatePlaceholders, 1000);

    });

    // Listening Mic Popup JS Start
