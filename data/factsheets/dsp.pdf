    function showShareOptions(ele){
        if ($(ele).hasClass('share-icon-popover') == false){
            $(ele).children().children().attr('fill', '#fff')
            $(ele).addClass('share-icon-popover rounded-circle')
        }
        else {
            $(ele).children().children().attr('fill', '#003A5D')
            $(ele).removeClass('share-icon-popover rounded-circle')
        }
    }

    $('[data-bs-toggle="popover"]').on('inserted.bs.popover', function () {
        $('.clipboard').unbind( 'click' ); 
        $('.clipboard').on('click', function(e) { 
            e.preventDefault(); 
            let $temp = $('<input>'); 
            $('body').append($temp); 
            $temp.val($(this).attr('href')).select(); 
            document.execCommand('copy'); 
            $temp.remove(); 
            // console.log('URL copied!'); 
            $(this).find('span').text('Link Copied'); 
        }); 
    });

