                }, 600);
                $("#coachmarkBox").hide();
            })
        });

        // Close search popup when clicked outside the popup
        $(document).on("click", function(event) {
            if (!$(event.target).closest('.dsp-search-popup-content').length) {
                $(".search-popup").removeClass("active");
                $("body").removeClass("no-scroll");
                document.getElementById('searchPlaceholderInput1').disabled = false;
                $("#coachmarkBox").hide();
            }
        });
    });

    async function handleSearchInput(event) {

        const typed_data = event.target.value;
        if (typed_data.trim().length > 2) {

            document.querySelector(".dsp-trending-wrap").style.display = "none";
            document.querySelector(".dsp-search-filter-main").style.display = "none";
            document.getElementById("coachmarkBox").style.display = "none";
            document.querySelector(".dsp-folio-wrap").style.display = "none";

            if (/\b\d{5,}\b/.test(typed_data.trim())) {
                document.querySelector(".dsp-folio-wrap").style.display = "block";
                document.querySelector(".dsp-search-bar-details").style.display = "none";
                $('.dsp-no-result-found-wrap').show();
                adjustHeight();
                return;
            }

            const header_token = document.querySelector('input[name="header_token"]');

            // Get the updated query parameters
            const queryParams = updateQuery(typed_data);

            // Fetch data with updated query parameters
            api_data = await get_search_data(queryParams, header_token.value);

            createSearchResults(api_data, typed_data, queryParams);
        } else {
            document.querySelector(".dsp-trending-wrap").style.display = "block";
            document.querySelector(".dsp-search-filter-main").style.display = "none";
            document.getElementById("coachmarkBox").style.display = "none";
            document.querySelector(".dsp-folio-wrap").style.display = "none";
        }
        //height for animation
        adjustHeight();
    }

    document.getElementById('searchPlaceholderInput2').addEventListener('input', handleSearchInput);
    document.getElementById('searchPlaceholderInput2').addEventListener('input', resetFilters);

    async function get_search_data(queryParams, header_token) {
        const formData = new FormData();
        formData.append("query", queryParams);
        formData.append("header_token", header_token);

        try {
            const response = await fetchApi('', 'POST', formData);

            if (!response.ok) {
                throw new Error('Network response was not ok');
            }

            const responseData = await response.json();

            if (responseData.allResults) {
                if (responseData.allResults == 'reload') {
                    location.reload();
                }
                return responseData;
            } else {
                throw new Error('Server returned an unexpected response');
            }
        } catch (error) {
            // Handle errors
            console.error('There was a problem with the API request: ', error);
            return null;
        }
    }

    function updateQuery(typed_data) {
        const filters = {
            'nfo': 'n',
            'faqs': 'n',
            'blogs': 'n',
            'locations': 'n',
            'documents': 'n',
            'podcasts': 'n',
            'funds': 'n',
            'holdings': 'n'
        };

        document.querySelectorAll('.scheme-category-filter').forEach(function(filter) {
            const filterName = filter.querySelector('.pr-filter-name').textContent.toLowerCase();
            const isActive = !filter.classList.contains('add-filter');

            if (filterName === 'others' && !isActive) {
                setTimeout(() => {
                    document.querySelectorAll('.dsp-search-title').forEach(function(titleElement) {
                        if (titleElement.textContent.trim() === 'Others') {
                            const sectionElement = titleElement.closest('.dsp-search-details-inner');
                            if (sectionElement) {
                                sectionElement.remove();
                            }
                        }
                    });
                }, 800);

            }

            if (isActive) {
                filters[filterName] = 'y';
            } else {
                filters[filterName] = 'n';
            }

        });

        const queryParams = new URLSearchParams({
            query: typed_data,
            ...filters
        }).toString();
        return queryParams;

    }

    function filterOthers(otherSearches, typed_data) {
        return otherSearches.filter(other => {
            const typedLower = typed_data.toLowerCase();

            // Check if the typed data matches the title
            const matchesTitle = other.title.toLowerCase().includes(typedLower);

            // Check if the typed data matches any alias
            const matchesAlias = other.alias.some(aliasObj => aliasObj.alias.toLowerCase().includes(typedLower));

            // Return true if either the title or alias matches
            return matchesTitle || matchesAlias;
        });

    }

    const debouncedToggleDisplay = debounce(() => {
        if (data_present) {
            toggleClearSearchIcon();
            if (searchInput.value.trim().length == 0) {
                document.querySelector(".no-result-search-btn").style.display = "none";
                document.getElementById("coachmarkBox").style.display = "none";

            } else {
                document.querySelector(".dsp-search-filter-main").style.display = "block";
                showCoachmarkOnce();
            }
        } else {
            document.querySelector(".dsp-search-filter-main").style.display = "none";
            document.querySelector(".no-result-search-btn").style.display = "block";
            document.querySelector(".clear-search-icon").style.display = "block";

            document.querySelector(".dsp-trending-wrap").style.display = "block";
        }
        //height for animation
        adjustHeight();
    }, 500);

    function section_display(section, visibility) {
        const sectionId = section.toLowerCase() + '_filter';
        const element = document.getElementById(sectionId);

        if (element) {
            element.style.display = visibility;
        }
    }

    async function createSearchResults(api_data, typed_data, queryParams) {

        const container = document.getElementById('searchResultsContainer');
        container.innerHTML = ''; // Clear any existing content

        const sections = {
            NFO: 'NFO',
            FUNDS: 'Funds',
            OTHERS: 'Others',
            DOCUMENTS: 'Documents',
            FAQS: 'FAQs',
            BLOGS: 'Blogs',
            PODCASTS: 'Podcasts',
            LOCATIONS: 'Locations',
            HOLDINGS: 'Holdings'

        };

        data_present = false;

        var queryObject = queryParams.split('&').reduce((acc, item) => {
            let [key, value] = item.split('=');
            acc[key] = value;
            return acc;
        }, {});

        // Loop through the sections
        for (let section in sections) {
            let sectionData = [];

            if (section === 'OTHERS') {

                sectionData = filterOthers(otherSearches, typed_data);

                if (sectionData.length > 0) {
                    section_display(section, 'block');
                } else {
                    section_display(section, 'none');
                }

            } else if (api_data.allResults[section] && api_data.allResults[section].length > 0) {
                sectionData = api_data.allResults[section];
                section_display(section, 'block');
            } else if (queryObject[section.toLowerCase()] === 'y') {
                section_display(section, 'none');
            }

            if (sectionData.length > 0) {

                const sectionTitle = sections[section];

                const sectionElement = document.createElement('div');
                sectionElement.className = 'dsp-search-details-inner';

                const titleElement = document.createElement('h3');
                titleElement.className = 'dsp-search-title';
                titleElement.textContent = sectionTitle;
                sectionElement.appendChild(titleElement);

                if (section !== 'DOCUMENTS') {
                    sectionData.sort((a, b) => {
                        return a.title.localeCompare(b.title);
                    });
                }
                sectionData.forEach(item => {

                    data_present = true;

                    const itemElement = document.createElement('div');
                    itemElement.className = 'dsp-search-details-inner-flex';

                    const itemDescElement = document.createElement('div');
                    itemDescElement.className = 'dsp-search-details-inner-flex-item';

                    const linkElement = document.createElement('a');
                    linkElement.className = 'dsp-search-details-inner-flex-item-desc';
                    linkElement.href = window.location.origin + item.link;
                    linkElement.textContent = item.title.replace(/\u003E|\n|"|'/g, '').trim();
                    linkElement.onclick = samepagereload;

                    // Event listener to store recent search on click
                    linkElement.addEventListener('click', function() {
                        addRecentSearch(typed_data);
                        trackExternalEvents('search_mainsearch', {
                            'page_url': 'https://www.dspim.com/downloads',
                            'label': item.title,
                            'searchurl': window.location.origin + item.link,
                            'category': section,
                            'description': 'View',

                        });
                    });

                    itemDescElement.appendChild(linkElement);
                    itemElement.appendChild(itemDescElement);

                    const actionElement = document.createElement('div');
                    actionElement.className = 'dsp-search-icon-box';

                    if (section === 'DOCUMENTS' || section === 'FAQS' || section === 'BLOGS' || section === 'PODCASTS' || section === 'OTHERS' || section === 'LOCATIONS' || section === 'HOLDINGS') {
                        const iconContainer = document.createElement('div');
                        iconContainer.className = 'dsp-search-icon-right';

                        const hamburgerIcon = document.createElement('i');
                        hamburgerIcon.className = 'dsp-search-icon-dots';
                        actionElement.appendChild(hamburgerIcon);

                        const eyeIcon = document.createElement('a');
                        eyeIcon.href = window.location.origin + item.link;
                        eyeIcon.innerHTML = '<i class="feather-eye"></i>';
                        eyeIcon.onclick = samepagereload;

                        if (section === 'DOCUMENTS') {
                            eyeIcon.target = '_blank';
                        }

                        eyeIcon.addEventListener('click', function() {
                            addRecentSearch(typed_data);
                            trackExternalEvents('search_mainsearch', {
                                'page_url': 'https://www.dspim.com/downloads',
                                'label': item.title,
                                'searchurl': window.location.origin + item.link,
                                'category': section,
                                'description': 'View',

                            });
                        });
                        iconContainer.appendChild(eyeIcon);

                        if (section === 'DOCUMENTS') {
                            const linkIcon = document.createElement('a');
                            linkIcon.href = 'javascript:void(0);'; // Prevent default behavior
                            linkIcon.innerHTML = '<i class="feather-link"></i>';

                            linkIcon.addEventListener('click', function() {
                                navigator.clipboard.writeText(window.location.origin + item.link).then(() => {
                                    // Remove any existing ".src-lnk-copy" elements
                                    const existingMessage = this.querySelector('.src-lnk-copy');
                                    if (existingMessage) {
                                        existingMessage.remove();
                                    }

                                    // Create the span element
                                    const copyMessage = document.createElement('span');
                                    copyMessage.className = 'src-lnk-copy';
                                    copyMessage.textContent = 'Link copied to clipboard';

                                    // Append the span element to the link icon
                                    this.appendChild(copyMessage);

                                    // Remove the span element after 3 seconds
                                    setTimeout(() => {
                                        copyMessage.remove();
                                    }, 3000);
                                });

                                addRecentSearch(typed_data);
                                trackExternalEvents('search_mainsearch', {
                                    'page_url': 'https://www.dspim.com/downloads',
                                    'label': item.title,
                                    'searchurl': window.location.origin + item.link,
                                    'category': section,
                                    'description': 'Copy Link',
                                });
                            });

                            iconContainer.appendChild(linkIcon);

                            const downloadIcon = document.createElement('a');
                            downloadIcon.href = window.location.origin + item.link;
                            downloadIcon.innerHTML = '<i class="feather-download"></i>';
                            downloadIcon.download = ''; // Trigger download
                            downloadIcon.addEventListener('click', function() {
                                addRecentSearch(typed_data);
                                trackExternalEvents('search_mainsearch', {
                                    'page_url': 'https://www.dspim.com/downloads',
                                    'label': item.title,
                                    'searchurl': window.location.origin + item.link,
                                    'category': section,
                                    'description': 'Download',

                                });
                            });
                            iconContainer.appendChild(downloadIcon);
                        }

                        actionElement.appendChild(iconContainer);
                    } else {
                        const buttonElement = document.createElement('a');
                        buttonElement.href = window.location.origin + item.link;
                        buttonElement.className = 'dsp-searchbtn';
                        buttonElement.innerHTML = '<i class="feather-chevron-right"></i>';
                        description = 'Know More';

                        buttonElement.addEventListener('click', function() {
                            addRecentSearch(typed_data);
                            trackExternalEvents('search_mainsearch', {
                                'page_url': 'https://www.dspim.com/downloads',
                                'label': item.title,
                                'searchurl': window.location.origin + item.link,
                                'category': section,
